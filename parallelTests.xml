<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Parallel tests" parallel="tests" verbose="1">
    <test name="ChromeSignUpTestWithUrlAndProfilePointsVerification" thread-count="1" verbose="1">
        <parameter name="browser" value="Chrome"/>
        <parameter name="headless" value="false"/>
        <classes>
            <class name="tests.MyAccountTest"/>
        </classes>
    </test>

    <test name="FFSignUpTestWithUrlAndProfilePointsVerification" thread-count="1" verbose="1">
        <parameter name="browser" value="FireFox"/>
        <parameter name="headless" value="false"/>
        <classes>
            <class name="tests.MyAccountTest"/>
        </classes>
    </test>

    <test name="EdgeSignUpTestWithUrlAndProfilePointsVerification" thread-count="1" verbose="1">
        <parameter name="browser" value="Edge"/>
        <parameter name="headless" value="false"/>
        <classes>
            <class name="tests.MyAccountTest"/>
        </classes>
    </test>
</suite>